{
  "version": 2,
  "build": {
    "env": {
      "YARN": "true"
    }
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "PATCH", "DELETE", "HEAD", "POST", "PUT", "OPTIONS"]
    }
  ]
}
